{
  "name": "testfragilitylint",
  "displayName": "TestFragilityLint",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Activate",
        "title": "Lint Test Case for Fragility",
        "category": "Lint",
        "shortTitle": "Lint for Fragility"
      },
      {
        "command": "Generate Report",
        "title": "Generate Report for the Current File",
        "category": "Lint",
        "shortTitle": "Report"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "acorn-node": "^2.0.1",
    "java-parser": "^2.0.0",
    "selenium-webdriver": "^4.1.0"
  }
}
